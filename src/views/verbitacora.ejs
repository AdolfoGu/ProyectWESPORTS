     <link href="https://fonts.googleapis.com/css2?family=Gemunu+Libre:wght@300;500;600;700&display=swap" rel="stylesheet">
</head>
<body class="fondo">


  <div class="site-mobile-menu site-navbar-target">
    <div class="site-mobile-menu-header">
      <div class="site-mobile-menu-close mt-3">
        <span class="icon-close2 js-menu-toggle"></span>
      </div>
    </div>
    <div class="site-mobile-menu-body"></div>
  </div>
  
  <header class="site-navbar site-navbar-target py-4" role="banner">
    <div class="container">
      <div class="row align-items-center position-relative">
        <div class="col-3">
          <div class="site-logo">
            <a href="/catalogo" class="font-weight-bold text-black"><img src="/img/logo.png" width="250" height="60"></a>
          </div>
        </div>
        <div class="col-9  text-right">
          <span class="d-inline-block d-lg-block"><a href="#" class="text-black site-menu-toggle js-menu-toggle py-5"><span class="icon-menu h3 text-black"></span></a></span>
          <nav class="site-navigation text-right ml-auto d-none d-lg-none" role="navigation">
            <ul class="site-menu main-menu js-clone-nav ml-auto ">
              <li><a href="/catalogo" class="nav-link">CATALOGO</a></li>
              <li><a href="/inventario" class="nav-link">INVENTARIO</a></li>
              <li class="active"><a href="/dashboard" class="nav-link">BIT√ÅCORA</a></li>
              <li><a href="/verventas" class="nav-link">VENTAS</a></li>
              <li><a href="/profile" class="nav-link">PERFIL</a></li>
              <li><a href="/" class="nav-link">SALIR</a></li>
            </ul>
          </nav>
        </div>
      </div>
    </div>
  </header>
<br><br><br><br>


<!--Tabla V2-->
<div class="content">
    
    <div class="container">
    <% for(var i = 0; i < products.length; i++){ %>
      <h2 class="mb-5">Bitacora numero: <%= products[i].Pedido %></h2>

      <div class="table-responsive">

        <table class="table custom-table">
          <thead>
            <tr>
              <th scope="col">
              </th>
              <th scope="col">
            </th>
              <th scope="col">Elaboro: <%= products[i].Nombre%></th>
              <th scope="col">Cliente: <%= products[i].Cliente %></th>
              <th scope="col">Fecha: <%= products[i].Fecha %></th>
              <th scope="col"></th>
            </tr>
          </thead>
          <% } %>
          <thead>
            <% for(var i = 0; i < tshirt.length; i++){ %>
            <tr>
              <th scope="col">
              </th>
              <th scope="col">
            </th>
              <th scope="col">Codigo:</th>
              <th scope="col">Descripcion:</th>
              <th scope="col">Talla:</th>
              <th scope="col">Cantidad:</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row">
              </th>
              <td>
              </td>
              <td><%= tshirt[i].modelo%></td>
                <td><%= tshirt[i].descripcion %></td>
                <td><%= tshirt[i].talla %></td>
                <td><%= tshirt[i].cantidad %></td>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <div class="mx-auto" style="width: 200px;">
    <a href="/dashboard" class="btn btn-danger align-items: center">
        Regresar
      </a>
  </div>
<script src="/js/jquery-3.3.1.min.js"></script>
<script src="/js/popper.min.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/jquery.sticky.js"></script>
<script src="/js/main.js"></script>
</body>
</html>