<body class="fondo">
  <%if (signinMessage.length > 0) {%>
      <script>
        Swal.fire({
          icon: 'error',
          title: 'Oops...',
          text: '<%= signinMessage %>'
        })
      </script>
  <% } %>
  <div class="limiter">
    <div class="container-login100">
      <div class="wrap-login100">
        <div class="login100-form-title" style="background-image: url(../img/2.png);">
          <span class="login100-form-title-1">
            Iniciar Sesión
          </span>
        </div>
        <form action="index" method="POST" class="login100-form validate-form">
          <div class="wrap-input100 validate-input m-b-26" data-validate="Username is required">
            <span class="label-input100">Correo</span>
            <input class="input100" type="email" name="email" placeholder="Ingresa tu correo">
            <span class="focus-input100"></span>
          </div>
          <div class="wrap-input100 validate-input m-b-18" data-validate = "Password is required">
            <span class="label-input100">Contraseña</span>
            <input class="input100" type="text" name="password" placeholder="Ingresa tu contraseña">
            <span class="focus-input100"></span>
          </div>
            <div>
              <br>
              <a href="/signup" class="txt1">
                ¿No tienes una cuenta? Registrate
              </a>
            </div>
            <br><br><br>
            <div class="container-login100-form-btn">
              <button type="submit" class="login100-form-btn" >
                Iniciar Sesión
              </button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div> 
